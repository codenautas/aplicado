{
    "name": "aplicado",
    "description": "Noticias de ayer",
    "version": "0.0.1",
    "dependencies": {
        "express": "^4.17.1",
        "require-bro": "~0.1.13",
        "serve-content": "~0.3.12"
    },
    "devDependencies": {
        "typescript": "^3.9.3",
        "@types/node": "^14.0.1",
        "@types/express": "^4.17.6",
        "@types/mocha": "^7.0.2",
        "@types/puppeteer": "^3.0.0",
        "mocha": "^7.1.2",
        "puppeteer": "^3.1.0",
        "types.d.ts": "~0.5.0",
        "open": "^7.0.4"
    },
    "scripts": {
        "prepare": "tsc -p tsconfig-server.json && tsc -p tsconfig-client.json && tsc -p tsconfig-test.json",
        "watch:buildC": "tsc --watch -p tsconfig-client.json",
        "watch:buildS": "tsc --watch -p tsconfig-server.json",
        "watch:buildT": "tsc --watch -p tsconfig-test.json",
        "build-start": "npm run prepare && npm start",
        "start": "node dist-server/server/server-noticias.js",
        "test": "mocha dist-test/test/puppe.js"
    }
}
